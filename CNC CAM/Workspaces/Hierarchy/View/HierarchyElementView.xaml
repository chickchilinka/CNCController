<UserControl x:Class="CNC_CAM.Workspaces.Hierarchy.View.HierarchyElementView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:CNC_CAM.Workspaces.Hierarchy.View"
             mc:Ignorable="d"
             d:DesignHeight="auto" d:DesignWidth="auto">
    <Grid>
        <Grid.ContextMenu>
            <ContextMenu>
            <MenuItem Header="Удалить" Click="Delete_OnClick"></MenuItem>
            </ContextMenu>
        </Grid.ContextMenu>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto"></ColumnDefinition>
            <ColumnDefinition Width="Auto"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Image x:Name="Icon" Width="24px" Height="24px"> </Image>
        <Label x:Name = "Label" Grid.Column="1"/>
    </Grid>
</UserControl>
