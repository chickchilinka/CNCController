<Window x:Class="CNC_CAM.Configuration.View.ManageConfigurationWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:CNC_CAM.Configuration.View"
        xmlns:data="clr-namespace:CNC_CAM.Configuration.Data"
        mc:Ignorable="d"
        Title="Управление конфигурациями" Height="450" Width="800">
    <Grid HorizontalAlignment="Stretch">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="200px"></ColumnDefinition>
            <ColumnDefinition Width="*"> </ColumnDefinition>
        </Grid.ColumnDefinitions>
        <TreeView x:Name="TreeView" Margin="5" Height="Auto">
            
        </TreeView>
        <local:ManageConfigurationPanel Grid.Column="1" x:Name="ManageConfigurationPanel"></local:ManageConfigurationPanel>
    </Grid>
</Window>